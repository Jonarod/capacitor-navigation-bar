{
  "name": "@jonarod/capacitor-navigation-bar",
  "version": "0.0.1",
  "description": "Set navigation bar properties on Android.",
  "main": "dist/plugin.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "lint": "npm run prettier -- --check && npm run swiftlint -- lint",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "release": "git add . && git commit -m \"new release\" && git push -u origin master && npm publish --access=public"
  },
  "author": "Jonarod",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^2.4.3",
    "@capacitor/core": "^2.4.3",
    "@capacitor/ios": "^2.4.3",
    "@ionic/prettier-config": "^1.0.0",
    "@ionic/swiftlint-config": "^1.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "prettier": "^2.0.5",
    "prettier-plugin-java": "^0.8.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.21.0",
    "swiftlint": "^1.0.1",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "@capacitor/core": "^2.4.3"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "JonarodCapacitorNavigationBar.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "repository": {
    "type": "git",
    "url": "git@github.com:Jonarod/capacitor-navigation-bar.git"
  },
  "bugs": {
    "url": "git@github.com:Jonarod/capacitor-navigation-bar.git/issues"
  }
}
